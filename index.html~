<!--[if lte IE 8]><script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script><![endif]--> 

<script>
	$('#submit').click(function(){
		$(this).attr("disabled","disabled");
		$.ajax({
                    url: 'scripts/mailer.php',
                    type: 'POST',
                    dataType: 'json',
                    data: 'name=' + $("#name").val()+ '&email=' + $("#email").val() +"&comment="+$("#comment").val(),
                    global: false,
                    success: function (data, textStatus, jqXHR) {
                        if(data[0] == "success"){
                                alert(data[1]);

                                $("#contactfrm .form-group").removeClass("has-error");
                                $(".name-err, .email-err , .comment-err").html("");
                                $("#name, #email , #comment").val("");
                        } else if(data[0] == "error") {
                                alert(data[1]);

                                $("#contactfrm .form-group").removeClass("has-error");
                                $(".name-err, .email-err , .comment-err").html("");
                        } else if(data[0] == "bug") {
                                if(data[1] != undefined){
                                        $(".name-err").html(data[1]);
                                        $("#contactfrm .col-sm-4:nth-child(1) .form-group:nth-child(1)").addClass("has-error");
                                } else {
                                        $("#contactfrm .col-sm-4:nth-child(1) .form-group:nth-child(1)").removeClass("has-error");
                                        $(".name-err").html("");
                                }

                                if(data[2] != undefined){
                                        $(".email-err").html(data[2]);
                                        $("#contactfrm .col-sm-4:nth-child(1) .form-group:nth-child(2)").addClass("has-error");
                                } else {
                                        $("#contactfrm .col-sm-4:nth-child(1) .form-group:nth-child(2)").removeClass("has-error");
                                        $(".email-err").html("");
                                }

                                if(data[3] != undefined){
                                        $(".comment-err").html(data[3]);
                                        $("#contactfrm .col-sm-4:nth-child(2) .form-group:nth-child(1)").addClass("has-error");
                                } else {
                                        $("#contactfrm .col-sm-4:nth-child(2) .form-group:nth-child(1)").removeClass("has-error");
                                        $(".comment-err").html("");
                                }
                        }
                        $('#submit').removeAttr("disabled");
                    },
                    error: function (jqXHR, textStatus, errorThrown) {
                       // $().toastmessage('showErrorToast', errorThrown);
                       $('#submit').removeAttr("disabled");
                       alert(errorThrown);
                    }
                });
	});
</script>
